m4trace:m4.include/mc-version.m4:11: -1- AC_DEFUN([mc_VERSION], [
    if test -f ${srcdir}/mc-version.h; then
        VERSION=$(grep '^#define MC_CURRENT_VERSION' ${srcdir}/mc-version.h | sed 's/.*"\(.*\)"$/\1/')
